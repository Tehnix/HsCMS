#archives {
    padding: 15px 30px 15px 30px;
    font-size: 0.9em;
    font-weight: 200;
    line-height: 1.5em;
    color: #444;
    
    table {
        tbody {
            tr {
                cursor: pointer;
                td {
                    a {
                        color: #444;
                    }
                    a:hover {
                        text-decoration: none;
                    }
                }
            }
        }
    }
}